<phyrex run.id=72 output.file="output/phyrex/LV/phyrex_output/out" mcmc.chain.len="2E+7" mcmc.sample.every="1000"
        mcmc.print.every="1000" mcmc.burnin="10000" mutmap="no" ignore.sequences="yes">

  <!-- Tree topology -->
  <topology>
  	 <instance id="T1" init.tree="user" file.name="output/phyrex/LV/phyrex_input/phyrex_tree72.txt" optimise.tree="no"/>
</topology>

               <!-- Model of rate variation across lineages -->
  <lineagerates model="lognormal"/>

  <!-- Average (clock) rate of substitution -->
  <clockrate value="1"/>
  
  
  <!-- Substitution model -->
  <ratematrices id="RM1">
    <instance id="M1" model="HKY85" optimise.tstv="no" tstv="4.0"/>
  </ratematrices>

  
    <!-- Freerate model of variation of rates across sites -->
  <siterates id="SR1">
    <instance id="R1" init.value="1.0"/>
    <weights  id="D1" family="gamma" optimise.freerates="no">
      <instance appliesto="R1" value="1.00"/>
    </weights>
  </siterates>

  <!-- Nucleotide frequencies -->
  <equfreqs id="EF1">
    <instance id="F1" optimise.freqs="no"/>
  </equfreqs>


  <!-- Vector of edge lengths -->
  <branchlengths id="BL1" >
    <instance id="L1" optimise.lens="no"/>
  </branchlengths>

  <!-- Model assembly -->
  <partitionelem id="partition1" file.name="output/phyrex/LV/phyrex_input/phyrex_nexus72.nxs" data.type="nt" interleaved="no">
    <mixtureelem list="T1"/>
    <mixtureelem list="M1"/>
    <mixtureelem list="F1"/>
    <mixtureelem list="R1"/>
    <mixtureelem list="L1"/>
  </partitionelem>
